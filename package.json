{
  "name": "okta-angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:docs": "compodoc -p ./tsconfig.json --theme Laravel -w -s --includes .",
    "build": "ng build",
    "test": "ng test",
    "test:docs": "compodoc -d ./public -p ./tsconfig.json --theme Laravel --coverageTest --coverageTestThresholdFail false  --includes .",
    "build:ci": "ng build --prod --sourceMap=false --base-href /OktaAngularApp/",
    "build:docs": "compodoc -d ./public -p ./tsconfig.json --theme Laravel  --includes .",
    "test:ci": "ng test --watch=false --code-coverage --source-map true",
    "lint": "ng lint",
    "e2e": "ng e2e --dev-server-target",
    "e2e:ci": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
